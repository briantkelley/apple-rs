{
    "cSpell.ignoreRegExpList": [
        "hexadecimal-number"
    ],
    "cSpell.ignoreWords": [
        "corefoundation",
        "deallocator",
        "fastpath",
        "objc",
        "repr",
        "slowpath",
        "uninit"
    ],
    "cSpell.patterns": [
        {
            "name": "hexadecimal-number",
            "pattern": "0[Xx][0-9A-Fa-f]+"
        }
    ],
    "rust-analyzer.check.command": "clippy"
}